<h1>ユーザー一覧</h1>
<table border="1" align="center">
  <tr>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>電話番号</th>
    <th colspan="3"></th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email%></td>
    <td><%= user.number%></td>
    <td><%= link_to "編集", edit_user_registration_path %></td>
  </tr>
<% end %>
</table>
